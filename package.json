{
	"name": "virusseq-dataportal-ui",
	"version": "1.14.9",
	"private": true,
	"scripts": {
		"build": "npm run nuke:nextTemp && next build",
		"dev": "npm run nuke:nextTemp && next dev",
		"dev:ssl-proxy": "local-ssl-proxy --key localhost.dev.virusseq-dataportal.ca-key.pem --cert localhost.dev.virusseq-dataportal.ca.pem --source 80 --target 3000",
		"dev:submission": "concurrently -n portal-ui,ssl-proxy -c blue,magenta -P 'npm run dev' 'npm run dev:ssl-proxy'",
		"link:Arranger": "npm link @overture-stack/arranger-components --ignore-scripts",
		"lint": "eslint --ext .ts,.tsx '.'",
		"patchTS": "ts-patch install -s",
		"nuke:nextTemp": "rimraf .next",
		"predev": "npm run nuke:nextTemp",
		"nuke:nodeModules": "rimraf node_modules",
		"reset": "npm run nuke:nextTemp && npm run nuke:nodeModules",
		"start": "next start",
		"test": "jest"
	},
	"dependencies": {
		"@emotion/babel-preset-css-prop": "^11.10.0",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@overture-stack/arranger-components": "^3.0.2",
		"@overture-stack/sqon-builder": "^1.1.0",
		"axios": "^0.27.2",
		"classnames": "^2.3.2",
		"date-fns": "^4.1.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"highcharts": "^9.0.1",
		"highcharts-react-official": "^3.1.0",
		"js-md5": "^0.8.3",
		"jsonwebtoken": "^8.5.1",
		"jszip": "^3.10.1",
		"jwt-decode": "^3.1.2",
		"lodash": "^4.17.21",
		"next": "^12.2.2",
		"next-global-css": "^1.3.1",
		"next-transpile-modules": "^9.0.0",
		"query-string": "^7.1.1",
		"react": "^17.0.2",
		"react-datepicker": "^4.25.0",
		"react-dom": "^17.0.2",
		"react-dropzone": "^11.3.2",
		"react-ga": "^3.3.1",
		"react-grid-system": "^7.1.2",
		"react-table": "^7.8.0",
		"react-tippy": "^1.4.0",
		"url-join": "^4.0.1",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@emotion/babel-plugin": "^11.10.5",
		"@emotion/eslint-plugin": "^11.10.0",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/lodash": "^4.14.191",
		"@types/node": "^22.14.1",
		"@types/react": "^17.0.47",
		"@types/react-datepicker": "^4.4.2",
		"@types/react-dom": "^17.0.17",
		"@types/react-table": "^7.7.14",
		"@types/url-join": "^4.0.1",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"concurrently": "^9.1.2",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.10.2",
		"eslint-import-resolver-babel-module": "^5.3.2",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^5.5.4",
		"extra-watch-webpack-plugin": "^1.0.3",
		"fork-ts-checker-webpack-plugin": "^7.3.0",
		"jest": "^29.4.1",
		"local-ssl-proxy": "^2.0.5",
		"next-compose-plugins": "^2.2.1",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"rimraf": "^6.0.1",
		"ttypescript": "^1.5.15",
		"typescript": "^5.9.3",
		"typescript-transform-paths": "^3.4.6"
	},
	"overrides": {
		"date-fns": ""
	},
	"prettier": {
		"organizeImportsSkipDestructiveCodeActions": true,
		"printWidth": 120,
		"semi": true,
		"singleAttributePerLine": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	}
}
